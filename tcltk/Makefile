	-rm -f Makefile
distclean-am: clean-am distclean-compile distclean-generic \
	distclean-tags

dvi: dvi-recursive

dvi-am:

html: html-recursive

info: info-recursive

info-am:

install-data-am:

install-exec-am: install-binPROGRAMS install-pcbexecDATA \
	install-pcbexecSCRIPTS

install-info: install-info-recursive

install-man:

installcheck-am:

maintainer-clean: maintainer-clean-recursive
	-rm -rf ./$(DEPDIR)
	-rm -f Makefile
maintainer-clean-am: distclean-am maintainer-clean-generic

mostlyclean: mostlyclean-recursive

mostlyclean-am: mostlyclean-compile mostlyclean-generic

pdf: pdf-recursive

pdf-am:

ps: ps-recursive

ps-am:

uninstall-am: uninstall-binPROGRAMS uninstall-info-am \
	uninstall-pcbexecDATA uninstall-pcbexecSCRIPTS

uninstall-info: uninstall-info-recursive

.PHONY: $(RECURSIVE_TARGETS) CTAGS GTAGS all all-am check check-am \
	clean clean-binPROGRAMS clean-generic clean-recursive ctags \
	ctags-recursive distclean distclean-compile distclean-generic \
	distclean-recursive distclean-tags distdir dvi dvi-am html \
	html-am info info-am install install-am install-binPROGRAMS \
	install-data install-data-am install-exec install-exec-am \
	install-info install-info-am install-man install-pcbexecDATA \
	install-pcbexecSCRIPTS install-strip installcheck \
	installcheck-am installdirs installdirs-am maintainer-clean \
	maintainer-clean-generic maintainer-clean-recursive \
	mostlyclean mostlyclean-compile mostlyclean-generic \
	mostlyclean-recursive pdf pdf-am ps ps-am tags tags-recursive \
	uninstall uninstall-am uninstall-binPROGRAMS uninstall-info-am \
	uninstall-pcbexecDATA uninstall-pcbexecSCRIPTS


all-am: tclpcb${SHDLIB_EXT} pcb.tcl pcbexec pcb

clean-generic:
	$(RM) pcb pcb.tcl tclpcb${SHDLIB_EXT} 

tclpcb${SHDLIB_EXT}: ${pcb_object_files} ${link_OBJECTS}
	$(RM) tclpcb${SHDLiB_EXT}
	$(CC) ${CFLAGS} ${SHLIB_CFLAGS} -o $@ ${LDDL_FLAGS} ${pcb_object_files} \
		${link_OBJECTS} ${SHLIB_LIB_SPECS} ${LDFLAGS} ${X_EXTRA_LIBS} \
		${EXTRA_LIBS} -lc ${LIBS}

pcbexec$(EXEEXT): pcbexec.o
	$(RM) pcbexec$(EXEEXT)
	$(CC) ${CFLAGS} ${CPPFLAGS} ${DEFS} $(INCLUDES) \
		pcbexec.c -o pcbexec$(EXEEXT) ${LIB_SPECS} \
		${LDFLAGS} ${LIBS}

pcb: pcb.sh.m4
	$(M4) $(M4_DEFS) ${CPPFLAGS} pcb.sh.m4 > $@
	chmod 0755 $@

pcb.tcl: pcb.tcl.m4
	$(M4) $(M4_DEFS) ${CPPFLAGS} pcb.tcl.m4 > $@
	chmod 0755 $@
# Tell versions [3.59,3.63) of GNU make to not export all variables.
# Otherwise a system limit (for SysV at least) may be exceeded.
.NOEXPORT:
